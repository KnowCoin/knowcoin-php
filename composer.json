{
    "name": "knowcoin/knowcoin-php",
    "description": "The KnowCoin PHP library simplifies integration with the KnowCoin API.",
    "type": "library",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "KnowCoin\\KnowCoinPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KnowCoin\\KnowCoinPhp\\Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.9",
        "tightenco/collect": "^9.52"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
