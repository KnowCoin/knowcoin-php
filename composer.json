{
  "name": "knowcoin/knowcoin-php",
  "version": "1.0.0",
  "type": "library",
  "minimum-stability": "stable",
  "description": "PHP Library for interacting with the KnowCoin API",
  "keywords": ["knowcoin"],
  "homepage": "https://github.com/KnowCoin/knowcoin-php",
  "license": "MIT",
  "authors": [
    {
      "name": "KnowCoin, Inc.",
      "email": "code@knowcoin.com",
      "homepage": "http://www.knowcoin.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "guzzlehttp/guzzle": "^7.9",
    "tightenco/collect": "^9.52"
  },
  "autoload": {
    "psr-0": {
      "Monolog": "src",
      "KnowCoin\\KnowCoinPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KnowCoin\\KnowCoinPhp\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "brianium/paratest": "^7.7",
    "phpunit/phpunit": "^11.5"
  },
  "scripts": {
    "test": "vendor/bin/paratest --processes=4"
  }
}
